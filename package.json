{
  "name": "fancy-test",
  "description": "extendable utilities for testing",
  "version": "1.0.1",
  "author": "Jeff Dickey @jdxcode",
  "bugs": "https://github.com/jdxcode/fancy-test/issues",
  "dependencies": {
    "lodash": "^4.17.4",
    "stdout-stderr": "^0.1.6"
  },
  "devDependencies": {
    "@anycli/tslint": "^0.2.5",
    "@types/chai": "^4.1.2",
    "@types/lodash": "^4.14.100",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.0",
    "@types/sinon": "^4.1.3",
    "chai": "^4.1.2",
    "chalk": "^2.3.0",
    "concurrently": "^3.5.1",
    "eslint": "^4.17.0",
    "eslint-config-anycli": "^1.3.2",
    "mocha": "^5.0.0",
    "sinon": "^5.0.0",
    "ts-node": "^4.1.0",
    "typescript": "^2.7.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib"
  ],
  "homepage": "https://github.com/jdxcode/fancy-test",
  "keywords": [
    "dxcli"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": "jdxcode/fancy-test",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "lint": "concurrently -p command \"eslint .\" \"tsc -p test --noEmit\" \"tslint -p test\"",
    "posttest": "yarn run lint",
    "prepublishOnly": "yarn run build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\""
  },
  "types": "lib/index.d.ts"
}
